#include <iostream>
#include <cstring>
 
int main()
{
    
    int size = 5;
    int *a = new int[size];
    
    for(int i = 0; i < size; ++i)
    {
        std::cin >> a[i];
    
    }
    for(int i = 0, end = size/2; i < end; ++i)
    {
    int temp = a[i];
    a[i] = a[size-1-i];
    a[size-1-i] = temp;
    }
    
    for(int i = 0; i < size; ++i)
    {
        std::cout << a[i];
    }
    return 0;
}

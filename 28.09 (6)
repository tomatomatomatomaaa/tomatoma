#include <iostream>

using namespace std;

int main()
{
    int arr[5] = {5, 4, 3, 4, 9};
    int min = arr[0];
    int max = arr[0];
    int index = 0;
    for(int i = 0; i < 5; ++i)
{
    cout << arr[i] << ", ";
    if(arr[i] > max)
    {
        max = arr[i];
        index = i+1;
        
    }
}
    
    cout << endl;
    cout << max << endl;
    cout << index;
    return 0;

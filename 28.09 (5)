#include <iostream>

using namespace std;

int main()
{
int arr[5] = {5, 4, 3, 4, 9};
int min = arr[0];
int max = arr[0];
for(int i = 0; i < 5; ++i)
{
cout<<arr[i] << ", ";
if(arr[i] < min)
{
min = arr[i];
}
}
for(int i = 0; i < 5; ++i)
{

if(arr[i] > max)
{
max = arr[i];
}
}
cout << endl;
cout<<max<<endl;
cout<<min<<endl;
cout<<max - min;
return 0;

}
